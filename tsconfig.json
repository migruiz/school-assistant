{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
            "@auth/*": [
        "./src/@auth/*"
      ],
      "@i18n": [
        "./src/@i18n/index"
      ],
      "@i18n/*": [
        "./src/@i18n/*"
      ],
      "@fuse/*": [
        "./app/src/@fuse/*"
      ],
      "@history*": [
        "./src/@history"
      ],
      "@mock-utils/*": [
        "./src/@mock-utils/*"
      ],
      "@schema": [
        "./src/@schema"
      ],
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
